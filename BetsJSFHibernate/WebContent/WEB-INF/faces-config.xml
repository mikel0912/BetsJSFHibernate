<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	<managed-bean>
		<managed-bean-name>queryquestion</managed-bean-name>
		<managed-bean-class>KlaseBean.QueryQuestionBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>createquestion</managed-bean-name>
		<managed-bean-class>KlaseBean.CreateQuestionBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>login</managed-bean-name>
		<managed-bean-class>KlaseBean.LoginRegisterBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>createevent</managed-bean-name>
		<managed-bean-class>KlaseBean.CreateEventBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>deleteevent</managed-bean-name>
		<managed-bean-class>KlaseBean.DeleteEventBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>Hasiera.xhtml</display-name>
		<from-view-id>/Hasiera.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Hasiera.xhtml</display-name>
		<from-view-id>/Hasiera.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>register</from-outcome>
			<to-view-id>/Register.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Login.xhtml</display-name>
		<from-view-id>/Login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>true</from-outcome>
			<to-view-id>/Main.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Register.xhtml</display-name>
		<from-view-id>/Register.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>true</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Main.xhtml</display-name>
		<from-view-id>/Main.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>create</from-outcome>
			<to-view-id>/CreateQuestion.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Main.xhtml</display-name>
		<from-view-id>/Main.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>query</from-outcome>
			<to-view-id>/QueryQuestion.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateQuestion.xhtml</display-name>
		<from-view-id>/CreateQuestion.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>closeToMain</from-outcome>
			<to-view-id>/Main.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>QueryQuestion.xhtml</display-name>
		<from-view-id>/QueryQuestion.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>closeToMain</from-outcome>
			<to-view-id>/Main.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Main.xhtml</display-name>
		<from-view-id>/Main.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logOut</from-outcome>
			<to-view-id>/Hasiera.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Main.xhtml</display-name>
		<from-view-id>/Main.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>createEvent</from-outcome>
			<to-view-id>/CreateEvent.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateEvent.xhtml</display-name>
		<from-view-id>/CreateEvent.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>closeToMain</from-outcome>
			<to-view-id>/Main.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Main.xhtml</display-name>
		<from-view-id>/Main.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>querySport</from-outcome>
			<to-view-id>/QuerySportEvent.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>QuerySportEvent.xhtml</display-name>
		<from-view-id>/QuerySportEvent.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>closeToMain</from-outcome>
			<to-view-id>/Main.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Login.xhtml</display-name>
		<from-view-id>/Login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>close</from-outcome>
			<to-view-id>/Hasiera.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/WEB-INF/facelets/templates/LogOut.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logOut</from-outcome>
			<to-view-id>/Hasiera.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>QueryQuestion.xhtml</display-name>
		<from-view-id>/QueryQuestion.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logOut</from-outcome>
			<to-view-id>/Hasiera.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>QuerySportEvent.xhtml</display-name>
		<from-view-id>/QuerySportEvent.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logOut</from-outcome>
			<to-view-id>/Hasiera.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateEvent.xhtml</display-name>
		<from-view-id>/CreateEvent.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logOut</from-outcome>
			<to-view-id>/Hasiera.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateQuestion.xhtml</display-name>
		<from-view-id>/CreateQuestion.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logOut</from-outcome>
			<to-view-id>/Hasiera.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Main.xhtml</display-name>
		<from-view-id>/Main.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>deleteEv</from-outcome>
			<to-view-id>/GertaeraEzabatu.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>GertaeraEzabatu.xhtml</display-name>
		<from-view-id>/GertaeraEzabatu.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>closeToMain</from-outcome>
			<to-view-id>/Main.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>GertaeraEzabatu.xhtml</display-name>
		<from-view-id>/GertaeraEzabatu.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logOut</from-outcome>
			<to-view-id>/Hasiera.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Main.xhtml</display-name>
		<from-view-id>/Main.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>showQuestions</from-outcome>
			<to-view-id>/ShowQuestion.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>ShowQuestion.xhtml</display-name>
		<from-view-id>/ShowQuestion.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>closeToMain</from-outcome>
			<to-view-id>/Main.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>ShowQuestion.xhtml</display-name>
		<from-view-id>/ShowQuestion.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>aukeratuta</from-outcome>
			<to-view-id>/QuestionsFromDate.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
